[% USE YAML %]
<article>
<header>
  <h1>
    Oops!
  </h1>
</header>
<section>
  <p class="lead text-danger">
    <b>[% error.http_code | html %]</b>
    <i>[% error.simple_message | html %]</i>
  </p>
  <p>That means something went wrong...</p>
  <pre>[% error.stack_trace.as_string | html %]</pre>
</section>
<footer>
</footer>
</article>
